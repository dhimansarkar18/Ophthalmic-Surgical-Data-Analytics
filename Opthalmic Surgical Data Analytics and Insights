      "source": [
        "#Grouping to see what impacts Surgery Cost (USD)\n",
        "#Age\n",
        "cataract.groupby('Age_Category').Surgery_Cost_USD.agg(['max','min','mean','std','var','count']).round(1)\n",
        "#Sex\n",
        "children_cat.groupby('Sex').Surgery_Cost_USD.agg(['max','min','mean','std','var','count']).round(1)\n",
        "#Surgery Type\n",
        "children_cat.groupby('Surgery_Type').Surgery_Cost_USD.agg(['max','min','mean','std','var','count']).round(1)\n",
        "#Hospital City\n",
        "children_cat.groupby('H_City').Surgery_Cost_USD.agg(['max','min','mean','std','var','count']).round(1)\n",
        "#Hospital Location\n",
        "children_cat.groupby('H_Location').Surgery_Cost_USD.agg(['max','min','mean','std','var','count']).round(1)\n",
        "#Surgery Day Time\n",
        "children_cat.groupby('Surgery_Day_Time').Surgery_Cost_USD.agg(['max','min','mean','std','var','count']).round(1)\n",
        "#Number of Visits\n",
        "children_cat.groupby('Number_of_Visits').Surgery_Cost_USD.agg(['max','min','mean','std','var','count']).round(1)\n",
        "#Year of Visit Date\n",
        "children_cat.groupby('Year').Surgery_Cost_USD.agg(['max','min','mean','std','var']).round(1)"
