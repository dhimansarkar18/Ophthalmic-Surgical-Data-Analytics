        "#Removing any unwanted entries\n",
        "#Taking out any negative age value\n",
        "df = df[df.Age >= 0]\n",
        "\n",
        "#Taking out 2008 entries (we only want 2014 to 2020)\n",
        "df = df[df.Year != 2008]"
      ],
